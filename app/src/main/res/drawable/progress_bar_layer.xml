<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background"
          android:drawable="@drawable/progress_shap" />

    <item android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <gradient android:startColor="@color/progress" android:centerColor="@color/progress"          android:endColor="@color/progress" android:angle="270" android:centerY="0.75" />
                <corners
                    android:topLeftRadius="@dimen/mediummdp"
                    android:bottomLeftRadius="@dimen/mediummdp"
                    android:topRightRadius="@dimen/mediummdp"
                    android:bottomRightRadius="@dimen/mediummdp"/>
            </shape>
        </clip>
    </item>
</layer-list>